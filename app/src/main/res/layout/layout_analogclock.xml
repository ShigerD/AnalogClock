<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.android.tiger.analogclocktest.LauncherClock
        xmlns:launcher="http://schemas.android.com/apk/res-auto/com.android.tiger.analogclocktest"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="90dp">

        <!--View-->
        <com.android.tiger.analogclocktest.AnalogClock
            android:id="@+id/analogclock"
            android:layout_width="wrap_content"
            android:layout_height="100dp"
            launcher:clock_dial="@drawable/clock_bg_image"
            launcher:clock_h="@drawable/clock_h_image"
            launcher:clock_m="@drawable/clock_m_image"
            launcher:clock_s="@drawable/clock_s_image"
            />

    </com.android.tiger.analogclocktest.LauncherClock>
</LinearLayout>